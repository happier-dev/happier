{
  "name": "@happier-dev/protocol",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tools/v2": {
      "types": "./dist/tools/v2/index.d.ts",
      "default": "./dist/tools/v2/index.js"
    },
    "./spawnSession": {
      "types": "./dist/spawnSession.d.ts",
      "default": "./dist/spawnSession.js"
    },
    "./rpc": {
      "types": "./dist/rpc.d.ts",
      "default": "./dist/rpc.js"
    },
    "./checklists": {
      "types": "./dist/checklists.d.ts",
      "default": "./dist/checklists.js"
    },
    "./capabilities": {
      "types": "./dist/capabilities.d.ts",
      "default": "./dist/capabilities.js"
    },
    "./socketRpc": {
      "types": "./dist/socketRpc.d.ts",
      "default": "./dist/socketRpc.js"
    },
    "./changes": {
      "types": "./dist/changes.d.ts",
      "default": "./dist/changes.js"
    },
    "./updates": {
      "types": "./dist/updates.d.ts",
      "default": "./dist/updates.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postinstall:real": "yarn --cwd ../agents -s build && yarn -s build",
    "postinstall": "node -e \"const fs=require('fs');const cp=require('child_process');const has=(p)=>{try{return fs.existsSync(p)}catch{return false}};if(!has('tsconfig.json')||!has('../agents/tsconfig.json'))process.exit(0);const yarn=process.platform==='win32'?'yarn.cmd':'yarn';const r=cp.spawnSync(yarn,['-s','postinstall:real'],{stdio:'inherit'});process.exit(r.status??1);\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@happier-dev/agents": "0.0.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
